{
  "name": "cdk-watchful-edge",
  "version": "0.6.0",
  "description": "Watching your CDK apps since 2019 - Edge",
  "awscdkio": {
    "twitter": "emeshbi"
  },
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "cdk": "cdk",
    "test": "jest"
  },
  "keywords": [
    "cdk",
    "cloudwatch",
    "monitoring"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.github.eladb.watchful",
        "maven": {
          "groupId": "com.github.eladb",
          "artifactId": "cdk-watchful"
        }
      },
      "python": {
        "distName": "cdk-watchful",
        "module": "cdk_watchful"
      }
    }
  },
  "author": {
    "email": "elad.benisrael@gmail.com",
    "name": "Elad Ben-Israel",
    "url": "https://github.com/eladb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eladb/cdk-watchful"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "1.46.0",
    "@types/jest": "^25.2.3",
    "@types/node": "14.0.13",
    "aws-cdk": "1.46.0",
    "aws-sdk": "^2.701.0",
    "jest": "^25.5.4",
    "jsii": "^1.7.0",
    "jsii-pacmak": "^1.7.0",
    "jsii-release": "^0.1.6",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.46.0",
    "@aws-cdk/aws-cloudwatch": "1.46.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.46.0",
    "@aws-cdk/aws-dynamodb": "1.46.0",
    "@aws-cdk/aws-events": "1.46.0",
    "@aws-cdk/aws-events-targets": "1.46.0",
    "@aws-cdk/aws-lambda": "1.46.0",
    "@aws-cdk/aws-sns": "1.46.0",
    "@aws-cdk/aws-sns-subscriptions": "1.46.0",
    "@aws-cdk/aws-sqs": "1.46.0",
    "@aws-cdk/core": "1.46.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "1.46.0",
    "@aws-cdk/aws-cloudwatch": "1.46.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.46.0",
    "@aws-cdk/aws-dynamodb": "1.46.0",
    "@aws-cdk/aws-events": "1.46.0",
    "@aws-cdk/aws-events-targets": "1.46.0",
    "@aws-cdk/aws-lambda": "1.46.0",
    "@aws-cdk/aws-sns": "1.46.0",
    "@aws-cdk/aws-sns-subscriptions": "1.46.0",
    "@aws-cdk/aws-sqs": "1.46.0",
    "@aws-cdk/core": "1.46.0"
  }
